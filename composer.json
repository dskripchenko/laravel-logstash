{
    "name": "dskripchenko/laravel-logstash",
    "description": "Logstash logging.",
    "license": "MIT",
    "homepage": "https://github.com/dskripchenko/laravel-logstash",
    "keywords": [
        "laravel",
        "logs",
        "logstash",
        "ELK"
    ],
    "authors": [
        {
            "name": "Denis Skripchenko",
            "email": "denskrp90@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "monolog/monolog": "^3.5",
        "laravel/framework": "10.*",
        "guzzlehttp/guzzle": "^7.5",
        "dskripchenko/php-array-helper": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dskripchenko\\LaravelLogstash\\": [
                "src/"
            ]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dskripchenko\\LaravelLogstash\\Providers\\LogstashServiceProvider"
            ]
        }
    }
}
